
  public static class Kata
    {
        public static object[] RemoveEveryOther(object[] arr)
        {
 int newSize = (arr.Length + 1) / 2; 
        object[] result = new object[newSize];

        int j = 0;
        for (int i = 0; i < arr.Length; i += 2)
        {
            result[j++] = arr[i];
        }

        return result;
        }
    }
